{
  "name": "limit-concurrency-decorator",
  "version": "0.2.0",
  "license": "ISC",
  "description": "Decorator to limit concurrency of async functions",
  "keywords": [
    "async",
    "concurrency",
    "decorator",
    "parallelism",
    "promise",
    "limit"
  ],
  "homepage": "https://github.com/JsCommunity/limit-concurrency-decorator",
  "bugs": "https://github.com/JsCommunity/limit-concurrency-decorator/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/JsCommunity/limit-concurrency-decorator.git"
  },
  "author": {
    "name": "Julien Fontanet",
    "email": "julien.fontanet@isonoe.net"
  },
  "preferGlobal": false,
  "main": "dist/",
  "bin": {},
  "files": [
    "dist/"
  ],
  "browserslist": [
    ">2%"
  ],
  "engines": {
    "node": ">=4"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.40",
    "@babel/core": "^7.0.0-beta.40",
    "@babel/plugin-proposal-decorators": "^7.0.0-beta.40",
    "@babel/preset-env": "^7.0.0-beta.40",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^21",
    "babel-plugin-lodash": "^3.3.2",
    "cross-env": "^5.0.5",
    "eslint": "^4.15.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^6.0.0",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "husky": "^0.14.3",
    "jest": "^21",
    "rimraf": "^2.6.1"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production babel --source-maps --out-dir=dist/ src/",
    "clean": "rimraf dist/",
    "commitmsg": "yarn test",
    "dev": "cross-env NODE_ENV=development babel --watch --source-maps --out-dir=dist/ src/",
    "dev-test": "jest --bail --watch",
    "prebuild": "yarn run clean",
    "predev": "yarn run clean",
    "prepublishOnly": "yarn run build",
    "pretest": "eslint --ignore-path .gitignore --fix .",
    "test": "jest"
  },
  "jest": {
    "collectCoverage": true,
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src"
    ],
    "testRegex": "\\.spec\\.js$"
  }
}
